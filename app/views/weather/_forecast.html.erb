
<li class="col-span-1 flex flex-col divide-y divide-gray-200 rounded-lg bg-gray-200 text-center shadow-sm">
  <div class="flex flex-1 flex-col p-8">
    <img class="mx-auto size-20 shrink-0 rounded-full" src=<%= forecast.dig("icon")%> alt="">
    <h3 class="mt-6 text-sm font-medium text-gray-900"><%= forecast.dig("name")%> </h3>
    <dl class="mt-1 flex grow flex-col justify-between">
      <dt class="sr-only">Temperature</dt>
      <dd class="text-3xl text-gray-900"><%= forecast.dig("temperature") %>&deg;</dd>
      <dt class="sr-only">Short Forecast</dt>
      <dd class="text-gray-700">
        <%= forecast.dig("shortForecast")%>
      </dd>
    </dl>
  </div>

</li>